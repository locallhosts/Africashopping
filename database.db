CREATE DATABASE PoisePMS;
USE PoisePMS;

CREATE TABLE Customers (
  CustomerID INT PRIMARY KEY AUTO_INCREMENT,
  CustomerName VARCHAR(255),
  ContactNumber VARCHAR(255),
  Email VARCHAR(255)
);

CREATE TABLE ProjectManagers (
  ProjectManagerID INT PRIMARY KEY AUTO_INCREMENT,
  ProjectManagerName VARCHAR(255),
  ContactNumber VARCHAR(255),
  Email VARCHAR(255)
);

CREATE TABLE Architects (
  ArchitectID INT PRIMARY KEY AUTO_INCREMENT,
  ArchitectName VARCHAR(255),
  ContactNumber VARCHAR(255),
  Email VARCHAR(255)
);

CREATE TABLE StructuralEngineers (
  StructuralEngineerID INT PRIMARY KEY AUTO_INCREMENT,
  StructuralEngineerName VARCHAR(255),
  ContactNumber VARCHAR(255),
  Email VARCHAR(255)
);

CREATE TABLE Projects (
  ProjectID INT PRIMARY KEY AUTO_INCREMENT,
  ProjectName VARCHAR(255),
  CustomerID INT,
  ProjectManagerID INT,
  ArchitectID INT,
  StructuralEngineerID INT,
  FOREIGN KEY (CustomerID) REFERENCES Customers(CustomerID),
  FOREIGN KEY (ProjectManagerID) REFERENCES ProjectManagers(ProjectManagerID),
  FOREIGN KEY (ArchitectID) REFERENCES Architects(ArchitectID),
  FOREIGN KEY (StructuralEngineerID) REFERENCES StructuralEngineers(StructuralEngineerID)
);


